        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
var s_data;
var str_s_data;
var str_e_data;
var str_Anno;
var str_Mese;

var MONTHS_OFFSET = 1		// mesi indietro
var oggi = new Date();
var primo_giorno_mese;

// Primo giorno del mese in corso
primo_giorno_mese = str2date("01" + date2str(oggi,"MMyyyy"),"ddMMyyyy");

// Se l'elaborazione viene fatta entro il giorno 20 usano i dati del mese precedente,
// altrimenti quelli del mese in corso
if (getDayNumber(oggi, "m") &lt;= 20)
{
	str_s_data = dateAdd(primo_giorno_mese,"m", -1 * MONTHS_OFFSET);
}
else
{
	str_s_data = dateAdd(primo_giorno_mese,"m", -1 * (MONTHS_OFFSET - 1));
}

// Per avere l'ultimo giorno dell'intervallo, si aggiunge un mese
// e si sottrae un giorno
str_e_data = dateAdd(str_s_data,"m", 1);
str_e_data = dateAdd(str_e_data,"d", -1);

// Conversione in stringa
str_Anno = date2str(str_s_data,"yy");
str_Mese = date2str(str_s_data,"MM");
str_s_data = date2str(str_s_data,"yyyy/MM/dd");
str_e_data = date2str(str_e_data,"yyyy/MM/dd");



//Alert("20" + str_Anno + " - " + str_Mese);
</jsScript_script>
