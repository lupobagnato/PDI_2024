if (inizio == "202YMM") {	  	
d = new Date();					// sysdate
d.setMonth(d.getMonth() - 1);	// un mese indietro

y = d.getFullYear();			// anno
m = d.getMonth() + 1;			// mese +1 per compensare la numerazione che inizia da 0


var anno_mese_prec = new java.text.SimpleDateFormat("yyyyMM").format(d)		// conversione e formatazione con una funzione Java

inizio = anno_mese_prec;
fine = anno_mese_prec;

parent_job.setVariable("YYYYMM_from",inizio);
parent_job.setVariable("YYYYMM_to",fine);

};



true;
</script>
