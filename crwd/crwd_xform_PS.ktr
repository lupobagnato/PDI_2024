    <sql>SELECT
NROCASO
, time_accettazione  
, time_visita        
, time_pronto        
, time_dimissione    
, case
	when time_visita is null then null
	else (time_visita - time_accettazione) * 24 * 60	-- in minuti
  end tempo_Attesa
, case
	when time_dimissione is null then null
	else (time_dimissione - time_accettazione) * 24 * 60	-- in minuti
  end tempo_Stazionamento
, case
	when time_dimissione is null then null
	when time_pronto is null then null
	else (time_dimissione - time_pronto) * 24			-- in ORE
  end tempo_Att_ricovero

/*
, case
	when stanza like '%OBI%' then 1
	else 0
  end flag_OBI
, case
	when dataaccettazione is not null and oraaccettazione is not null then dataaccettazione + substr(oraaccettazione,1,2)/24 + substr(oraaccettazione,4,2)/(24*60)
	else null
  end time_accettazione	
, case
	when data_visita is not null and ora_visita is not null then data_visita + substr(ora_visita,1,2)/24 + substr(ora_visita,4,2)/(24*60)
	else null
  end time_visita
, case
	when dt_pronto is not null and ora_pronto is not null then dt_pronto + substr(ora_pronto,1,2)/24 + substr(ora_pronto,4,2)/(24*60)
	else null
  end time_pronto
, case
	when data_dimissione is not null and ora_dimissione is not null then data_dimissione + substr(ora_dimissione,1,2)/24 + substr(ora_dimissione,4,2)/(24*60)
	else null
  end time_dimissione
, case
	when dt_pronto is not null then 1
	else 0
  end flag_Pronto
*/
FROM 
${buffer}
</sql>
