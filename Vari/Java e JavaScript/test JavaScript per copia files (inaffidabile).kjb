      <script>var File = java.io.File;
var Files = java.nio.file.Files;
var Paths = java.nio.file.Paths;
var StandardCopyOption = java.nio.file.StandardCopyOption;

//var sourceDir = parent_job.getVariable("source") + "\\" + parent_job.getVariable("subdir") ;   
var sourceDir = "C:\\minollo\\record_SeT\\20250109"
parent_job.getLogChannel().logBasic("sourceDir " + sourceDir);

var destDir = parent_job.getVariable("dest");  //"c:\\minollo\\Log\\tmp" ;
parent_job.getLogChannel().logBasic("destDir " + destDir);

var wild_file = parent_job.getVariable("wild_file");
var regex = new RegExp(wild_file);


var dir = new File(sourceDir);
parent_job.getLogChannel().logBasic("dopo dir " + dir.getAbsolutePath());
if (dir.isDirectory()) {
    var files = dir.listFiles();
parent_job.getLogChannel().logBasic("files.length " + " -> " + files.length);
    for (var i = 0; i &lt; files.length; i++) {
        var file = files[i];
        if (file.isFile() &amp;&amp; regex.test(file.getName())) {
            var sourcePath = Paths.get(file.getAbsolutePath());
            var destPath = Paths.get(destDir, file.getName());
            Files.copy(sourcePath, destPath, StandardCopyOption.COPY_ATTRIBUTES);
            parent_job.getLogChannel().logBasic("File copiato: " + sourcePath + " -> " + destPath);
        }
    }
}
else
{ 
	parent_job.getLogChannel().logBasic(dir.getAbsolutePath() + " non è una dir");
}
true;
</script>
